<GameField>:
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            source: 'sky.png'
            size: self.size
            pos: self.pos

    cols: 20
    rows: 10
    default_size: 100, 100
    default_size_hint: None, None
    size_hint_y: None
    size_hint_x: None
    size: self.minimum_size

<CorporationGame>:
    game_field: field
    gui: gui_id
    object_layer: obj
    worker_layer: worker

    orientation: 'vertical'
    GameField:
        id: field
    FloatLayout:
        id: obj
    FloatLayout:
        id: worker
    FloatLayout:
        id: gui_id
